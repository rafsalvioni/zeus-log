{
    "name": "rafsalvioni/zeus-log",
    "description": "Provides a log management API",
    "license": "BSD-3-Clause",
    "keywords": [
        "log", "zeus", "psr", "handler"
    ],
    "homepage": "https://github.com/rafsalvioni/zeus-log",
    "autoload": {
        "psr-4": {
            "Zeus\\Log\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ZeusTest\\Log\\": "test/"
        }
    },
    "require": {
        "php": ">=7.0",
        "psr/log": "^1.0",
        "rafsalvioni/zeus-stream": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "5.7.19"
    },
    "repositories": [
        {
            "type": "path",
            "url": "../zeus-*"
        }
    ],    
    "suggest": {
    },
    "extra": {
    }
}
